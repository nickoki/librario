<h2>New Post</h2>

<%= form_for [@post] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :link %>
  <%= f.text_field :link %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :link_content_date %>
  <%= f.text_field :link_content_date %>

  <%= f.label :user_id %>
  <%= f.text_field :user_id %>

  <!-- <%= f.label :created_at %>
  <%= f.text_field :created_at %>

  <%= f.label :update_at %>
  <%= f.text_field :update_at %> -->

  <%= f.submit %>
<% end %>
